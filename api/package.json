{
  "name": "ctn-asr-api",
  "version": "1.0.2",
  "description": "CTN Association Register API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/openapi.json dist/openapi.json && cp -r src/templates dist/templates",
    "prebuild": "npm run test:schema-contracts",
    "watch": "tsc --watch",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "start:dev": "npm run build && node dist/server.js",
    "docker:build": "docker build -t ctn-asr-api .",
    "docker:run": "docker run -p 8080:8080 --env-file .env ctn-asr-api",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:schema-contracts": "node tests/integration/schema-contract-tests.js",
    "test:schema-contracts:skip": "echo 'Skipping schema contract tests'",
    "test:api": "node ../tests/api/integration/run-api-tests.js",
    "test:api:ci": "node ../tests/api/integration/run-api-tests.js --ci",
    "test:deploy": "node ../tests/api/deployment/run-all.js",
    "test:deploy:quick": "node ../tests/api/deployment/run-all.js --quick",
    "test:deploy:smoke": "node ../tests/api/deployment/smoke.test.js",
    "test:deploy:health": "node ../tests/api/deployment/health.test.js",
    "test:deploy:security": "node ../tests/api/deployment/security.test.js",
    "test:deploy:workflows": "node ../tests/api/deployment/workflows.test.js",
    "test:deploy:m2m": "node ../tests/api/deployment/m2m.test.js"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/communication-email": "^1.0.0",
    "@azure/eventgrid": "^5.7.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.28.0",
    "@types/cheerio": "^0.22.35",
    "@types/swagger-ui-express": "^4.1.8",
    "applicationinsights": "^2.9.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "gremlin": "^3.7.4",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "parse-multipart-data": "^1.5.0",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^8.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}

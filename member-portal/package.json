  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "lint:report": "biome check . --reporter=json > reports/biome-report.json || true",
    "security:audit": "npm audit --json > reports/npm-audit.json || true",
    "security:audit:fix": "npm audit fix",
    "security:summary": "npm audit > reports/security-summary.txt || true",
    "security:aikido:scan": "aikido-api-client scan 1094319 $BASE_COMMIT $HEAD_COMMIT main --minimum-severity-level MEDIUM",
    "security:aikido:release": "aikido-api-client scan-release 1094319 $HEAD_COMMIT --minimum-severity-level HIGH",
    "review": "npm run lint && npm run security:audit && npm run security:summary",
    "ci": "biome ci . && npm audit --audit-level=high",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.19.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.30.1"
  }
}

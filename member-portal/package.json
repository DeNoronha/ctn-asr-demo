{
  "name": "ctn-member-portal",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^3.7.1",
    "@azure/msal-react": "^2.0.22",
    "@ctn/api-client": "file:../packages/api-client",
    "@mantine/core": "^8.3.6",
    "@mantine/dates": "^8.3.6",
    "@mantine/dropzone": "^8.3.6",
    "@mantine/form": "^8.3.6",
    "@mantine/hooks": "^8.3.6",
    "@mantine/notifications": "^8.3.6",
    "@tabler/icons-react": "^3.26.0",
    "@tanstack/react-table": "^8.21.3",
    "ajv": "^8.17.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.548.0",
    "mantine-datatable": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dom-factories": "^1.0.2",
    "react-i18next": "^16.0.0",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "lint:report": "biome check . --reporter=json > reports/biome-report.json || true",
    "security:audit": "npm audit --json > reports/npm-audit.json || true",
    "security:audit:fix": "npm audit fix",
    "security:summary": "npm audit > reports/security-summary.txt || true",
    "security:aikido:scan": "aikido-api-client scan 1094319 $BASE_COMMIT $HEAD_COMMIT main --minimum-severity-level MEDIUM",
    "security:aikido:release": "aikido-api-client scan-release 1094319 $HEAD_COMMIT --minimum-severity-level HIGH",
    "review": "npm run lint && npm run security:audit && npm run security:summary",
    "ci": "biome ci . && npm audit --audit-level=high",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.56.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.19.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.30.1"
  }
}

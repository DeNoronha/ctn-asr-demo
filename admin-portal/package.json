{
  "name": "admin-portal",
  "version": "0.1.6",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^4.24.1",
    "@azure/msal-react": "^3.0.20",
    "@ctn/api-client": "file:../packages/api-client",
    "@mantine/core": "^8.3.10",
    "@mantine/dates": "^8.3.10",
    "@mantine/dropzone": "^8.3.10",
    "@mantine/form": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@tabler/icons-react": "^3.35.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.0",
    "exceljs": "^4.4.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "mantine-datatable": "^8.3.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.2.1",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --run --reporter=junit --reporter=verbose --outputFile.junit=test-results-junit.xml",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report playwright-report",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "lint:report": "biome check . --reporter=json > reports/biome-report.json || true",
    "security:audit": "npm audit --json > reports/npm-audit.json || true",
    "security:audit:fix": "npm audit fix",
    "security:summary": "npm audit > reports/security-summary.txt || true",
    "security:aikido:scan": "aikido-api-client scan 1094319 $BASE_COMMIT $HEAD_COMMIT main --minimum-severity-level MEDIUM",
    "security:aikido:release": "aikido-api-client scan-release 1094319 $HEAD_COMMIT --minimum-severity-level HIGH",
    "review": "npm run lint && npm run security:audit && npm run security:summary",
    "ci": "biome ci . && npm audit --audit-level=high"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.56.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/axios": "^0.9.36",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.19.21",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.6",
    "dotenv": "^17.2.3",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.30.1"
  }
}
